{
  "name": "redux-code",
  "author": "Kirill Klenov <horneds@gmail.com>",
  "license": "ISC",
  "version": "0.4.2",
  "description": "Redux helpers for actions and reducers",
  "main": "dist/index.js",
  "module": "es/index.js",
  "types": "es/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/klen/redux-code.git"
  },
  "bugs": {
    "url": "https://github.com/klen/redux-code/issues"
  },
  "homepage": "https://github.com/klen/redux-code#readme",
  "keywords": [
    "redux",
    "redux-actions",
    "redux-reducers"
  ],
  "scripts": {
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src/*.ts --ignore src/types.ts --extensions .ts --out-dir lib ",
    "build:es": "babel src/*.ts --ignore src/types.ts --extensions .ts --out-dir es"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-typescript"
    ]
  },
  "dependencies": {
    "immer": "^9.0.12",
    "redux": "^4.1.2"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "prettier": "^2.5.1",
    "redux-testing": "^0.4.2",
    "redux-thunk": "^2.4.1",
    "ts-jest": "^27.1.3"
  },
  "prettier": {
    "semi": false,
    "trailingComma": "all",
    "singleQuote": true,
    "printWidth": 120,
    "tabWidth": 2,
    "arrowParens": "always"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src"
    ],
    "setupFiles": [
      "<rootDir>/jest.setup.ts"
    ]
  }
}
