{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint",
    "prettier"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended",
    "prettier"
  ],
  "env": {
    "es6": true,
    "jest": true,
    "node": true
  },
  "rules": {
    "no-console": "warn",
    "no-debugger": "error",
    "prettier/prettier": "warn"
  }
}
